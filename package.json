{
  "name": "amork-ui",
  "version": "1.0.0",
  "type": "module",
  "description": "This is a mini vue3+vite UI libaray written in Typescritp.",
  "license": "MIT",
  "keywords": [
    "vue3",
    "components",
    "libaray"
  ],
  "files": [
    "dist"
  ],
  "module": "dist/es/amork-ui.js",
  "exports": {
    ".": {
      "import": "./dist/es/amork-ui.js"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build-es && npm run move-style",
    "build-es": "vite build --config vite.es.config.ts",
    "move-style": "move-file dist/es/style.css dist/style.css",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@popperjs/core": "^2.11.8",
    "lodash-es": "^4.17.21",
    "move-file-cli": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.25"
  }
}
